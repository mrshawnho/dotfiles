set defaultnewtabpage
let searchengine gg="https://www.google.com/search?q="
let searchengine gh="https://github.com/search?q="
let searchengine bd="https://www.baidu.com/s?wd="
let searchengine zh="https://www.zhihu.com/search?type=content&q="
let searchengine sf="https://segmentfault.com/search?q="
let searchengine yd="http://dict.youdao.com/w/"
